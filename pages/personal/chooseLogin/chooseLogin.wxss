page {
    padding-bottom: 50rpx;
    background: #fff;
}

.logo {
    padding-top: 258rpx;
}

.logoImg {
    width: 100%;
    height: 110rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.logo-img {
    width: 110rpx;
    height: 110rpx;
}

.logoTxt {
    margin-top: 23rpx;
    font-size: 28rpx;
    color: #584f4f;
    line-height: 40rpx;
    text-align: center;
}

.btn {
    text-align: center;
    width: 690rpx;
    margin: 145rpx auto 0;
}

.btn .btnWx {
}

.btn .btnWx button {
    padding-left: 0;
    padding-right: 0;
    box-sizing: border-box;
    font-size: 30rpx;
    line-height: 72rpx;
    text-align: center;
    text-decoration: none;
    height: 72rpx;
    border-radius: 8rpx;
    overflow: hidden;
    color: #ff5000;
    background-color: #ffe1d3;
}

.btn .btnWx button::after {
    border: none;
}

.btn .btnHi {
    text-align: right;
    font-size: 24rpx;
    margin-top: 28rpx;
    color: #aca3a3;
}

.tipslogin {
    position: fixed;
    bottom: 80rpx;
    height: 26rpx;
    line-height: 26rpx;
    font-size: 24rpx;
    color: #999;
    width: 100%;
    text-align: center;
}

.icon-xy {
    width: 26rpx;
    height: 26rpx;
    margin-right: 14rpx;
    vertical-align: middle;
    position: relative;
    margin-top: -6rpx;
}