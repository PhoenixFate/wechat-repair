.tianxiedizhi {
    width: 686rpx;
    padding: 0 42rpx;
    margin: 12rpx auto 0;
    box-sizing: border-box;
    background: #fff;
    box-shadow: 0px 3px 24px 0px rgba(163,163,163,0.2);
    border-radius: 10px;
}

.row {
    display: flex;
    border-bottom: 1rpx solid #e8e8e8;
    min-height: 100rpx;
    max-height: 300rpx;
    height: auto;
    font-size: 28rpx;
    color: #000;
    line-height: 100rpx;
    justify-content: space-between;
    align-items: center;
    padding-left: 64rpx;
}

.row:last-child {
    border-bottom: none;
    padding-left: 0;
}

.row input {
    height: 100rpx;
    line-height: 100rpx;
    width: 420rpx;
}

.name {
    background: url(data:image/png;base64,) left center no-repeat;
}

.phone {
    background: url(data:image/png;base64,) left center no-repeat;
}

.city {
    background: url(data:image/png;base64,) left center no-repeat;
}

.detailAddress {
    background: url(data:image/png;base64,) left center no-repeat;
}

.detailAddress textarea {
    line-height: 38rpx;
    max-height: 300rpx;
}

.name,.phone,.city,.detailAddress {
    background-size: 32rpx 32rpx;
}

.btnsure {
    width: 686rpx;
    height: 100rpx;
    border-radius: 10rpx;
    background: #fff;
    box-shadow: 0px 3px 24px 0px rgba(163,163,163,0.2);
    color: #FF7A00;
    line-height: 100rpx;
    text-align: center;
    position: fixed;
    bottom: 30rpx;
    left: 50%;
    margin-left: -343rpx;
}

.input {
    padding-top: 300rpx;
    width: 100%;
}

.input input {
    width: 100%;
    background-color: #fff;
    border-bottom: 1px #d9d9d9 solid;
    border-top: 1px #d9d9d9 solid;
    padding: 20rpx 50rpx;
}

.section {
    padding-top: 20rpx;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

.modal {
    height: 100%;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.3);
    z-index: 1;
}

.container {
    position: absolute;
    height: 100%;
    width: 100%;
    min-height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0;
    box-sizing: border-box;
    background-color: #f8f8f8;
}

.citypicker {
    position: fixed;
    bottom: 0px;
    z-index: 3;
    background: #fff;
    font-size: 28rpx;
    text-align: center;
    transform: translateY(150%);
    border-radius: 4rpx 4rpx 0 0;
    height: 300px;
    width: 100%;
}

.citybody {
}

.cityheader {
    width: 100%;
    background: #fff;
    border-radius: 4rpx 4rpx 0 0;
    height: 80rpx;
}

.city-cancel {
    float: left;
    margin: 20rpx;
    color: #818181;
}

.city-true {
    float: right;
    margin: 20rpx;
    color: #2FB42E;
}

.section .picker {
    background-color: #fff;
    border-bottom: 1px #d9d9d9 solid;
    border-top: 1px #d9d9d9 solid;
    padding: 20rpx;
}