page {
    background: #fff;
}

.search {
    display: block;
    background: #fff;
    position: fixed;
    top: 0;
    margin-left: -343rpx;
    left: 50%;
}

.search .searchInput {
    width: 686rpx;
    margin: 0 auto;
    padding-left: 80rpx;
    line-height: 76rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #d1d1d1;
    border-radius: 10rpx;
    background: url(http://pic.hiweixiu.com/images/miniprogram/search.png) 28rpx center no-repeat;
    background-color: #f7f7f7;
    background-size: 34rpx 34rpx;
}

.wodeshebei {
    position: fixed;
    width: 100%;
    height: 62rpx;
    margin-bottom: 20rpx;
    background: #ffedc3 url("https://pic.hiweixiu.com/hiweixiu-app/weapp/img/17020413.png") no-repeat 718rpx center;
    line-height: 62rpx;
    text-align: center;
    color: #333;
    font-size: 26rpx;
    background-size: 11rpx 20rpx;
}

.content {
    position: fixed;
    left: 0;
    display: flex;
    border-top: 10rpx solid #F7F7F7;
    border-bottom: 1rpx solid #F7F7F7;
    background: #fff;
}

.top0 {
    top: 0rpx;
}

.top90 {
    top: 100rpx;
}

.sidebar {
    width: 179rpx;
    min-width: 179rpx;
    max-width: 179rpx;
    height: auto;
    flex-grow: 0;
    background: #f5f5f5;
}

.menu {
    width: 180rpx;
    height: 100rpx;
    text-align: center;
}

.menu.hi_logo {
    background: #ffffff;
    margin-bottom: 10rpx;
}

.hi_logo image {
    width: 180rpx;
    height: 100rpx;
}

.menu .txt.brand {
    position: relative;
}

.menu .txt.brand::after {
    content: "";
    width: 93rpx;
    height: 34rpx;
    position: absolute;
    top: -8rpx;
    right: 0;
    background: url(data:image/png;base64,) center center no-repeat;
    z-index: 999;
    background-size: 80rpx 28rpx;
}

.menu .txt.brand {
    line-height: 70rpx;
}

.menu.on .txt {
    color: #010101;
}

.menu .txt {
    display: block;
    height: 100rpx;
    border-left: 4rpx solid #f9f9f9;
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    color: #ababab;
    position: relative;
    background: #f5f5f5;
    transition: 1s;
}

.menu.on .txt {
    background-color: #fff;
}

.menu.on .yuan {
    display: inline-block;
    position: absolute;
    margin-left: 8rpx;
    top: 25rpx;
    width: 14rpx;
    height: 14rpx;
    background: url("https://pic.hiweixiu.com/hiweixiu-app/weapp/img/170204019.png") no-repeat 0 0;
    background-size: cover;
}

.tabs {
    background: #fff;
    box-shadow: 0px 3rpx 24rpx 0rpx rgba(163,163,163,0.2);
}

.tabsContainer {
    height: 100rpx;
    padding: 0 30rpx;
}

.onlyPhone {
    display: none;
}

.fl {
    float: left;
}

.cl::after {
    display: block;
    content: "";
    clear: both;
}

.nav {
    width: 150rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    float: left;
    color: #666;
    position: relative;
    margin: 0;
    padding: 0;
    background: none;
}

.nav::after {
    content: none;
}

.nav.on {
    color: #FF7A00;
    background-size: contain;
}

.right {
    flex-grow: 2;
}

.list {
    width: 570rpx;
    clear: both;
}

.item {
    width: 570rpx;
    height: 128rpx;
    display: flex;
    align-items: center;
}

.item .img {
    width: 80rpx;
    height: 80rpx;
    margin: 0 48rpx;
}

.item .con {
    height: 128rpx;
    line-height: 128rpx;
    font-size: 26rpx;
    color: #000;
    display: block;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.maskView {
    position: fixed;
    z-index: 9999;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(255,255,255,1);
    overflow: hidden;
}

.img {
    opacity: 0;
}