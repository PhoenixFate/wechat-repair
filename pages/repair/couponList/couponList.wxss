page {
    background: #F5F5F5;
}

.coupon {
    width: 686rpx;
    margin: 0 auto;
    overflow: hidden;
}

.coupon .couponInput {
    width: 686rpx;
    height: 100rpx;
    background: #fff;
    box-shadow: 0px 3rpx 24rpx 0rpx rgba(163,163,163,0.2);
    border-radius: 10rpx;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 32rpx;
    font-size: 28rpx;
    position: fixed;
    top: 12rpx;
    margin-left: -343rpx;
    left: 50%;
}

.coupon .couponInput input {
    margin-left: 32rpx;
    width: 460rpx;
}

.coupon .couponInput .couponUseBtn {
    width: 180rpx;
    height: 100rpx;
    background: #FF7A00;
    border-radius: 10rpx;
    color: #fff;
    text-align: center;
    line-height: 100rpx;
}

.couponListItem_body {
    display: flex;
    width: 100%;
    height: 93px;
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: cover;
    box-shadow: 0px 6px 65px 0px rgba(184,184,184,0.23);
    margin-bottom: 30rpx;
}

.couponListItem_body .type {
    width: 110rpx;
    padding: 36rpx;
    font-size: 36rpx;
    color: #fff;
    line-height: 56rpx;
    box-sizing: border-box;
    position: relative;
}

.couponListItem_body .type .active {
    height: 48rpx;
    width: 24rpx;
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 24rpx 48rpx;
    position: absolute;
    top: 60rpx;
    left: 0;
}

.couponListItem_body .content {
    width: 510rpx;
    height: 100%;
    padding: 0 36rpx;
    box-sizing: border-box;
}

.couponListItem_body .content .contentMain {
    height: 128rpx;
    padding-top: 12rpx;
    border-bottom: 2rpx solid #ececec;
    box-sizing: border-box;
}

.couponListItem_body .content .contentMain .contentTitle {
    font-size: 34rpx;
    color: #FF7A00;
}

.couponListItem_body .content .contentMain .contentCode {
    font-size: 24rpx;
    color: #363636;
}

.couponListItem_body .content .contentMain .contentRule {
    font-size: 24rpx;
    color: #ababab;
}

.couponListItem_body .content .couponDate {
    color: #ababab;
    font-size: 24rpx;
    padding-top: 4rpx;
}

.couponListItem_body .title {
    width: 72rpx;
    padding: 30rpx 22rpx;
    font-size: 28rpx;
    color: #fff;
    box-sizing: border-box;
}

.noUseBtn {
    width: 686rpx;
    height: 100rpx;
    background: #fff;
    box-shadow: 0px 3rpx 24rpx 0rpx rgba(163,163,163,0.2);
    border-radius: 10rpx;
    overflow: hidden;
    margin-bottom: 32rpx;
    font-size: 28rpx;
    position: fixed;
    bottom: 30rpx;
    margin-left: -343rpx;
    left: 50%;
    text-align: center;
    line-height: 100rpx;
    color: #FF7A00;
}