.arrow {
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 15rpx 27rpx;
}

.fault_item_child .child_item .child_item_img.active {
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 40rpx 40rpx;
    border: none;
}

.choose_fault {
    overflow: hidden;
}

.top {
    position: relative;
}

.top,.top_bg,.top_bg image {
    height: 336rpx;
    width: 750rpx;
}

.top_btn {
    position: absolute;
    top: 23rpx;
    right: 40rpx;
    display: flex;
    align-items: center;
    width: 120rpx;
    height: 40rpx;
}

.top_btn .icon,.top_btn .icon image {
    width: 30rpx;
    height: 30rpx;
    margin-top: -5rpx;
}

.top_btn .text {
    font-size: 22rpx;
    font-weight: 500;
    color: #584F4F;
    line-height: 30rpx;
}

.main {
    width: 750rpx;
    margin-top: -143rpx;
    position: relative;
    z-index: 2;
}

.main .content {
    width: 694rpx;
    margin: 0 auto;
    padding: 24rpx;
    padding-bottom: 0;
    box-sizing: border-box;
    background: #ffffff;
    box-shadow: 0rpx 12rpx 24rpx 0rpx rgba(0,0,0,0.08);
    border-radius: 8rpx;
}

.main .model {
    display: flex;
    align-items: center;
    padding-bottom: 25rpx;
    border-bottom: 1px solid #e4e0de;
}

.main .model .model_img {
    margin-right: 22rpx;
}

.main .model .model_img,.main .model .model_img image {
    width: 120rpx;
    height: 120rpx;
}

.main .model .model_name {
    font-size: 30rpx;
    color: #584f4f;
    line-height: 42rpx;
}

.main .model .model_change {
    margin-left: auto;
    width: 120rpx;
    height: 42rpx;
    background: #ffede5;
    border-radius: 8rpx;
    font-size: 24rpx;
    color: #ff5000;
    line-height: 42rpx;
    text-align: center;
}

.main .color {
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.main .color .color_name {
    margin-left: 3rpx;
}

.main .color .color_name .no_choosed {
    font-size: 28rpx;
    color: #aca3a3;
    margin-left: 7rpx;
}

.main .color .color_name .choosed {
    display: flex;
    align-items: center;
}

.main .color .color_name .choosed .color_bg,.color_picker .color_bg {
    height: 34rpx;
    width: 34rpx;
    border-radius: 50%;
    box-sizing: border-box;
}

.main .color .color_name .choosed .color_bg.isblack,.color_picker .color_bg.isblack {
    border: 1px solid #000;
}

.main .color .color_name .choosed .color_text {
    margin-left: 17rpx;
    font-size: 28rpx;
    color: #584f4f;
}

.main .color .color_arrow {
    width: 18rpx;
    height: 30rpx;
}

.warranty {
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    color: #584f4f;
    line-height: 40rpx;
}

.warranty_left {
    margin-left: 7rpx;
}

.baoxiu_switch {
    color: #ababab;
    font-size: 26rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.baoxiu_switch switch {
    zoom: .7;
    margin-left: 12rpx;
}

.fault {
    margin-top: 54rpx;
    margin-bottom: 110rpx!important;
}

.fault_item {
    width: 690rpx;
    margin: 0 auto;
    border-bottom: 1px solid #E4E0DE;
    overflow: hidden;
}

.fault_item:last-child {
    border-bottom: none;
}

.fault_item_parent {
    display: flex;
    align-items: flex-start;
    margin: 0 auto;
    width: 690rpx;
    padding: 30rpx 12rpx 30rpx 18rpx;
    box-sizing: border-box;
}

.fault_item_parent .parent_img {
    margin-right: 21rpx;
}

.fault_item_parent .parent_img.official_logo_img {
    position: relative;
}

.fault_item_parent .parent_img.official_logo_img::after {
    content: '';
    position: absolute;
    top: -7rpx;
    right: -12rpx;
    width: 50rpx;
    height: 25rpx;
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 49rpx 24rpx;
}

.fault_item_parent .parent_img,.fault_item_parent .parent_img image {
    width: 98rpx;
    height: 98rpx;
}

.fault_item_parent .parent_fault .fault_type {
    display: flex;
    align-items: center;
}

.parent_right_top {
    width: 542rpx;
    justify-content: space-between;
    display: flex;
    align-items: center;
}

.parent_right_top .fault_item_parent .parent_fault .fault_type .fault_type_text {
    line-height: 42rpx;
    font-size: 30rpx;
    color: #584f4f;
}

.fault_type_num {
    margin-left: 6rpx;
    width: 34rpx;
    height: 34rpx;
    border-radius: 50%;
    background: linear-gradient(90deg,#ff6186 0%,#ff5000 100%);
    font-size: 24rpx;
    text-align: center;
    line-height: 34rpx;
    color: #ffffff;
}

.fault_des {
    line-height: 33rpx;
    font-size: 24rpx;
    color: #aca3a3;
    margin-top: 9rpx;
    max-width: 542rpx;
}

.fault_item_parent .parent_price {
    margin-left: auto;
    font-size: 30rpx;
    color: #ff5000;
    white-space: nowrap;
    display: flex;
    align-items: center;
}

.fault_item_parent .fault_arrow {
    height: 27rpx!important;
    width: 27rpx!important;
    transform: rotate(-180deg);
    margin-left: 8rpx;
    transform-origin: center center;
    transition: .3s;
    display: flex;
    justify-content: center;
    align-items: center;
}

.fault_item_parent .fault_arrow image {
    height: 15rpx;
    width: 27rpx;
}

.fault_item_parent .fault_arrow.fault_arrow_show {
    transform: rotate(0deg);
}

.child_content {
    box-sizing: border-box;
}

.child_show {
    display: block;
    transition: .3s linear;
    transform: scaleY(1);
    transform-origin: center top;
    padding-bottom: 32rpx;
}

.child_hidden {
    height: 0;
    transform: scaleY(0);
    transform-origin: center top;
    padding-bottom: 0rpx;
    transition: .1s linear;
}

.fault_item_child {
    margin: 0 auto;
    width: 690rpx;
    background: #f7f5f4;
    border-radius: 4rpx;
}

.fault_item_child .child_item {
    display: flex;
    align-items: center;
    padding: 25rpx 23rpx;
}

.fault_item_child .child_item .child_item_img {
    width: 40rpx;
    height: 40rpx;
    border: 1px solid #aea7a7;
    border-radius: 50%;
    box-sizing: border-box;
}

.fault_item_child .child_item .child_item_detail {
    height: 37rpx;
    font-size: 26rpx;
    color: #444242;
    margin-left: 15rpx;
}

.fault_item_child .child_item .child_item_price {
    margin-left: auto;
    font-size: 26rpx;
    color: #444242;
}

.bottom {
    width: 750rpx;
    display: flex;
    height: 110rpx;
    background: #fff;
    box-shadow: 0px -5px 17px 0px rgba(0,0,0,0.06);
    position: fixed;
    bottom: 0;
    left: 50%;
    transform: translate(-50%,0);
    z-index: 9;
}

.bottom .bottom_main {
    margin: 19rpx auto;
    width: 694rpx;
    height: 72rpx;
    background: rgba(255,225,212,1);
    border-radius: 8rpx;
    display: flex;
    align-items: center;
}

.bottom .bottom_main .total {
    width: 443rpx;
    display: flex;
    height: 72rpx;
    align-items: center;
}

.bottom .bottom_main .total .price {
    margin-left: 34rpx;
    color: #FF5000;
    font-size: 30rpx;
    font-weight: 600;
}

.bottom .bottom_main .total .price text {
    font-size: 26rpx;
}

.bottom .bottom_main .total .fault_num {
    color: #584F4F;
    font-size: 24rpx;
}

.bottom .bottom_main .btn {
    width: 251rpx;
    height: 72rpx;
}

.bottom .bottom_main .btn .confirm_btn {
    width: 251rpx;
    height: 72rpx;
    font-size: 30rpx;
    background: linear-gradient(90deg,#ff854d 0%,#ff5000 100%);
    border-radius: 0rpx 8rpx 8rpx 0rpx;
    line-height: 72rpx;
    color: #ffffff;
    text-align: center;
}

.bottom .bottom_main .btn button {
    position: relative;
    display: block;
    margin-left: 0;
    margin-right: 0;
    padding-left: 0;
    padding-right: 0;
    box-sizing: border-box;
    text-align: center;
    text-decoration: none;
    border-radius: 0;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #fff;
    background: linear-gradient(90deg,#ff854d 0%,#ff5000 100%);
}

.bottom .bottom_main .btn button::after {
    border: none;
}

.bottom .bottom_main .btn .login_btn {
    width: 251rpx;
    height: 72rpx;
    font-size: 30rpx;
    line-height: 72rpx;
    background: linear-gradient(90deg,#ff854d 0%,#ff5000 100%);
    border-radius: 0rpx 8rpx 8rpx 0rpx;
}

.popUpModel {
    height: 100%;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.3);
    z-index: 15;
    display: none;
}

.popUpModel.hide {
    display: block;
}

.drawer_dialog {
    width: 100%;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 600;
    transition: transform 0.3s;
    transition: transform 0.3s,-webkit-transform 0.3s;
    border-radius: 10px 10px 0px 0px;
    padding: 0px 16px 0;
    box-sizing: border-box;
    transform: translateY(100%);
}

.drawer_dialog .color_btn {
    width: 694rpx;
    height: 72rpx;
    background: linear-gradient(90deg,#ffede5 0%,#ffe0d2 100%);
    border-radius: 8rpx;
    font-size: 30rpx;
    color: #ff5000;
    line-height: 72rpx;
    text-align: center;
    margin-bottom: 20rpx;
}

