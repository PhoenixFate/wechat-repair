.fulishe_container {
    width: 100%;
    background: #f7f5f4;
    min-height: 100vh;
}

.fulishe_container .fulishe_title {
    width: 100%;
    height: 192rpx;
}

.fulishe_container .type_wrap {
    padding: 20rpx 20rpx 0;
    box-sizing: border-box;
    width: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
}

.fulishe_container .type_wrap .type {
    margin-right: 33rpx;
    width: 108rpx;
    height: 44rpx;
    background: white;
    border-radius: 4rpx;
    border: 1rpx solid #aca3a3;
    color: #C7C0BC;
    font-size: 28rpx;
    text-align: center;
    line-height: 44rpx;
}

.fulishe_container .type_wrap .type.active {
    color: #584F4F;
    border: 1rpx solid #584f4f;
}

.fulishe_container .nocoupon {
    margin-top: 40rpx;
    font-size: 30rpx;
    text-align: center;
}

.fulishe_container .scrollView.fullscreen {
    padding: 40rpx 20rpx;
}

.fulishe_container .scrollView {
    padding: 40rpx 20rpx 0;
    box-sizing: border-box;
}

.fulishe_container .scrollView .coupon_container {
    width: 100%;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap {
    padding: 0 25rpx 0 50rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    background: url("https://pic.hiweixiu.com/hiweixiu-mobile/img/weixiucoupon.png") no-repeat;
    background-size: 710rpx 180rpx;
    width: 710rpx;
    height: 180rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .price_wrap {
    font-size: 24rpx;
    width: 190rpx;
    -ms-flex-negative: 0;
    flex-shrink: 0;
    margin-top: 12rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .price_wrap .price {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: baseline;
    -ms-flex-align: baseline;
    align-items: baseline;
    color: #ff5000;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .price_wrap .price .rate {
    font-size: 66rpx;
    font-weight: bold;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .price_wrap .price .sym {
    color: #ff5000;
    font-size: 36rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .price_wrap .condition {
    margin-top: -10rpx;
    color: #ACA3A3;
    font-size: 24rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .use_condition_wrap {
    font-size: 24rpx;
    width: 340rpx;
    -ms-flex-negative: 0;
    flex-shrink: 0;
    -ms-flex-item-align: center;
    -ms-grid-row-align: center;
    align-self: center;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .use_condition_wrap view {
    color: #ACA3A3;
    width: 340rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .use_condition_wrap .name {
    color: #444242;
    font-size: 30rpx;
    font-weight: bold;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .btn {
    font-size: 24rpx;
    width: 108rpx;
    height: 42rpx;
    background: -webkit-gradient(linear,left top,right top,from(#ff854d),to(#ff5000));
    background: linear-gradient(90deg,#ff854d 0%,#ff5000 100%);
    border-radius: 8rpx;
    text-align: center;
    line-height: 42rpx;
    color: #fff;
    -ms-flex-item-align: center;
    -ms-grid-row-align: center;
    align-self: center;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .lingguang {
    color: #FF5000;
    font-size: 26rpx;
    font-weight: 600;
    -ms-flex-item-align: center;
    -ms-grid-row-align: center;
    align-self: center;
    width: 108rpx;
    height: 42rpx;
    text-align: center;
    line-height: 42rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap .shiyongnow {
    color: #FF5000;
    font-size: 24rpx;
    width: 106rpx;
    height: 42rpx;
    border-radius: 8rpx;
    border: 1rpx solid #ff5000;
    line-height: 42rpx;
    text-align: center;
    -ms-flex-item-align: center;
    -ms-grid-row-align: center;
    align-self: center;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap button.btn {
    padding: 0;
    border-radius: 8rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap button.btn::after {
    border: none;
}

.fulishe_container .scrollView .coupon_container .coupon_wrap.recyclecoupon_wrap {
    background: url("https://pic.hiweixiu.com/hiweixiu-mobile/img/huishouocoupon.png") no-repeat;
    background-size: 710rpx 180rpx;
}